{
  "name": "ngx-extensions",
  "description": "A set of usefull reusable angular elements",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run start:demo",
    "start:demo": "ng serve",
    "build:demo": "ng build",
    "test:lib": "ng test extensions --watch=false --code-coverage",
    "lint:lib": "ng lint extensions",
    "build:lib": "ng build --prod extensions",
    "package": "npm run build:lib && npm run npm:pack",
    "npm:pack": "cd dist/@ngx-extensions/extensions && npm pack",
    "generate:changelog": "conventional-changelog -i CHANGELOG.md -s -p angular"
  },
  "bugs": {
    "url": "https://github.com/ngx-extensions/ngx-extensions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-extensions/ngx-extensions.git"
  },
  "keywords": [
    "angular",
    "extensions",
    "reusable"
  ],
  "license": "MIT",
  "author": {
    "email": "jotatoledon@gmail.com",
    "name": "JosÃ© Toledo",
    "url": "https://github.com/jotatoledo"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5",
    "yarn": ">=1.3.2"
  },
  "private": true,
